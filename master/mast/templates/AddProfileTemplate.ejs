<div class="add-profile-template profile-list">

	<div class="title-mod">
		<div class="title-fix">
 			<span> Create New Profile </span>
			<div class="clear"></div>
		</div>
	</div>

<!-- settings content -->
	<div class="main-mod" id="main-mod"> 
		<!-- company details section -->
		<form action="" method="POST" class="new-user-form">
			<div class="center-mod">
				<!-- <h2 class="company-details">Create Profile</h2> -->
				<div class="company-name">&nbsp;</div>
				<div class="company-name">Profile Name</div>
					<input type="text" name="profile_name" placeholder="Enter Profile Name" class="enter_name profile_name">

				<div class="company-name">&nbsp;</div>
				<div class="company-name">Select Access Rights</div>
				<div class="company-name">&nbsp;</div>

				<div class="company-name">
					<input type ="checkbox" name="user_managment"> 
					User Managment
				</div>
				
				<div class="company-name">
					<input type ="checkbox" name="subscription">
					 Subscription Management
				</div>
				
				<div class="company-name">
					<input type ="checkbox" name="enterprises_management">
					 Enterprises Management
				</div>
				
				<div class="company-name">
					<input type ="checkbox" name="workgroup_managment_of_users"> 
					 User Workgroup Managment
				</div>
				
				<div class="company-name">
					<input type ="checkbox" name="workgroup_managment_of_enterprises">  Enterprises Workgroup Managment
				</div>
				
				<div class="company-name">
					<input type ="checkbox" name="manage_admins"> 
					 Manage Admins
				</div>

				<div class="company-name">
					<input type ="checkbox" name="manage_superadmins"> 
					 Manage SuperAdmins
				</div>

				<div class="company-name">
					<input type ="checkbox" name="manage_admin_user"> Manage Admin User
				</div>
			</div>  
		</form>
		<div class="clear"></div> 
	</div> 

	<!-- save button -->
	<div class="footer-mod">
		<a class="profile-save-button">
			<img src="/images/button_save@2x.png" />
		</a>
	</div>
	
</div>










