<div class="drive-consent-template">
	
	<div class="popup-head">Drive Consent URL</div>
	<div class="popup-body">

		<label class="files-text">Authorize this app by visiting this url: </label>
		<div class='upload-path-name'> <a class="files-text" href="{{ authorizeUrl }}" target="_blank">{{ authorizeUrl }}</a> </div>

		<div class="clear-tab"></div>
		<label class="files-text">Enter the code from that page here:</label>
		<input type="text" placeholder="" class="popup-box-input drive-box-input" />
	</div>

	<div class="popup-foot clearfix">
		<a class="upload-button no-submit-removed">Submit Code</a>
		<span class="cancel">Cancel</span>
		<div class="loading-spinner"></div>
	</div>

</div>