    <body>

        <!-- main content -->
        <div class="wrapper portal-only">
            <!-- top section with navigation and search -->
            <% if(header_color !== 'undefined') { %>
            <div id="topbar" style="background-color:<%= header_color %>">
                <% }else{ %>
                <div id="topbar">
                    <% } %>	
                    <div id="top-nav" class="clearfix">

                        <!-- logo and main navigation -->
                        <div id="main-nav">

                            <% if(enterprise_logo != '' && enterprise_logo != null){ %>
                            <a href="#" class="logo-link"><img class="main-logo" src="/images/enterprises/<%= enterprise_logo %>" alt="Olympus" /></a>
                            <% }else{ %>
                            <a href="#" class="logo-link"><img class="main-logo" src="/images/Branding_12.png" alt="Olympus" /></a>
                            <%	} %>

                            <ul class="clearfix">

                                <% if(is_super_admin != '1'){ %>

                                <li>
                                    <a href="#" title="overview" style="background-color:<%=header_color %>;border:1px solid <%=header_color %>">
                                        <div class="main-nav-div">
                                            <img class="main-nav-imgs" src="/images/Top-Nav_OverviewIcon@2x.png"/>
                                            <span class="main-nav-span">Overview</span>
                                        </div>
                                    </a>
                                </li>

                                <%  if(apps != '1'){ 	%>
                                <li>
                                    <a href="#listusers" title="users" style="background-color:<%=header_color %>;border:1px solid <%=header_color %>">
                                        <div class="main-nav-div special-user-topbar-fix">
                                            <img class="main-nav-imgs" src="/images/Top-Nav_UsersIcon@2x.png"/>
                                            <span class="main-nav-span">Users</span>
                                        </div>
                                    </a>
                                </li>

                                <li>
                                    <a href="#reports" title="reports" style="background-color:<%=header_color %>;border:1px solid <%=header_color %>">
                                        <div class="main-nav-div">
                                            <img class="main-nav-imgs" src="/images/Top-Nav_ReportsIcon@2x.png"/>
                                            <span class="main-nav-span">Add User</span>
                                        </div>
                                    </a>
                                </li> 

                                <% } %>							

                                <li class="last-item">
                                    <a href="#settings" title="settings" style="background-color:<%=header_color %>;border:1px solid <%=header_color %>">
                                        <div class="main-nav-div">
                                            <img class="main-nav-imgs" src="/images/Top-Nav_SettingsIcon@2x.png"/>
                                            <span class="main-nav-span">Settings</span>
                                        </div>
                                    </a>
                                </li> 

                                <% }else{ %>

                                <li class="last-item">
                                    <a href="#enterprises/1" title="enterprises" style="background-color:<%=header_color %>;border:1px solid <%=header_color %>;width:auto;">
                                        <div class="main-nav-div">
                                            <img class="main-nav-imgs" src="/images/enterprises.png"/>
                                            <span class="main-nav-span">Enterprises</span>
                                        </div>
                                    </a>
                                </li> 

                                <li class="last-item">
                                    <a href="#listusers/1" title="users"  style="background-color:<%=header_color %>;border:1px solid <%=header_color %>;width:auto;">
                                        <div class="main-nav-div">
                                            <img class="main-nav-imgs" src="/images/user_small.png"/>
                                            <span class="main-nav-span">List Users</span>
                                        </div>
                                    </a>
                                </li> 

                                <li class="last-item">
                                    <a href="#subscription" title="subscription plan"   style="background-color:<%=header_color %>;border:1px solid <%=header_color %>;width:auto;">
                                        <div class="main-nav-div">
                                            <img class="main-nav-imgs" src="/images/subscription_menu.png"/>
                                            <span class="main-nav-span">Subscription Plan</span>
                                        </div>
                                    </a>
                                </li>

                                <!--							<li class="last-item">
                                                                                                <a href="#profile" title="Create profile"  style="background-color:<%=header_color %>;border:1px solid <%=header_color %>;width:auto;">
                                                                                                        <div class="main-nav-div">
                                                                                                                <img class="main-nav-imgs" src="/images/manage_admin_profiles_small.png"/>
                                                                                                                <span class="main-nav-span">Manage Admin Profiles</span>
                                                                                                        </div>
                                                                                                </a>
                                                                                        </li> 
                                
                                                                                        <li class="last-item">
                                                                                                <a href="#adminUser" title="Manage Admin User"  style="background-color:<%=header_color %>;border:1px solid <%=header_color %>;width:auto;">
                                                                                                        <div class="main-nav-div">
                                                                                                                <img class="main-nav-imgs" src="/images/manage_admin_users_small.png"/>
                                                                                                                <span class="main-nav-span">Manage Admin Users</span>
                                                                                                        </div>
                                                                                                </a>
                                                                                        </li> -->

                                <li class="last-item">
                                    <a href="#log/1" title="View Log"  style="background-color:<%=header_color %>;border:1px solid <%=header_color %>;width:auto;">
                                        <div class="main-nav-div">
                                            <img class="main-nav-imgs" src="/images/manage_admin_users_small.png"/>
                                            <span class="main-nav-span">View Log</span>
                                        </div>
                                    </a>
                                </li>

                                <li class="last-item">
                                    <a href="#transaction/1" title="View Log"  style="background-color:<%=header_color %>;border:1px solid <%=header_color %>;width:auto;">
                                        <div class="main-nav-div">
                                            <img class="main-nav-imgs" src="/images/manage_admin_users_small.png"/>
                                            <span class="main-nav-span">View Payment Transaction</span>
                                        </div>
                                    </a>
                                </li>

                                <% } %>
                            </ul>
                        </div>

                        <!-- user navigaton outlet -->
                        <div id="user-nav-outlet" class="user-nav-outlet"></div>
                    </div>
                </div>

		<div style="float:right;padding:15px 37px 0px 0px;">
			<span class="main-nav-span">Total Space Used: <%- total_space_used %> GB</span>
		</div>
                <div id="content" style="background-color:<%= body_background %>"></div>
                <div class="push"></div>

            </div>

            <!-- footer -->
            <div id="footer" class="portal-only clearfix" style="background-color:<%= footer_background %>">

                <!-- Domain Information -->
                <div id="apppath" style="display:none;"><%= (typeof sails.config.appPath != 'undefined')?(sails.config.appPath).replace('master/config/..','api/files/'):'no path found :(' %></div>
                <div id="domaininfo" style="display:none;"><%= sails.config.host %></div>
                <div id="adapter_type" style="display:none;"> sails.config.fileAdapter.adapter</div>
                <div id="emailservice" style="display:none;"><%= sails.config.mailService %></div>
                <div id="mandrillkey" style="display:none;"><%= sails.config.mandrillApiKey %></div>
                <div id="smtphost" style="display:none;"><%= (typeof sails.config.smtpDetails != 'undefined')?sails.config.smtpDetails.host:'' %></div>
                <div id="smtpport" style="display:none;"><%= (typeof sails.config.smtpDetails != 'undefined')?sails.config.smtpDetails.port:'' %></div>
                <div id="smtpuser" style="display:none;"><%= (typeof sails.config.smtpDetails != 'undefined')?sails.config.smtpDetails.user:'' %></div>
                <div id="smtppass" style="display:none;"><%= (typeof sails.config.smtpDetails != 'undefined')?sails.config.smtpDetails.pass:'' %></div>
                <div id="trashopt" style="display:none;"><%= (typeof sails.config.trash_setting != 'undefined')?sails.config.trash_setting:'' %></div>
                <div id="trashoptdays" style="display:none;"><%= (typeof sails.config.trash_setting_days != 'undefined')?sails.config.trash_setting_days:'' %></div>
                <div id="body_bg_color" style="display:none;"><%= body_background %></div>
                <script type="text/javascript">
                    /*console.log(sails_config.express);
                    console.log(sails_config.session);
                     sails_config.express = null %>
                     sails_config.session = null %>
                    sailsConfig = JSON.parse(JSON.parse(JSON.stringify($('<div/>').html(" (JSON.stringify(sails_config))%>").text())));*/
                    // console.log(sailsConfig);
                </script>

                <div id="copyright-info">
                    <img id="footer-logo" class="footer-logo-class" src="/images/Branding_12.png" />
                    <span class="copyright-info"><%= sails.config.organization.copyright %></span>
                </div>
                <ul id="terms-policies" class="clearfix">
                    <li><a target="_blank" href="<%= sails.config.organization.links.termsOfUse %>">Terms of Use</a>|</li>
                    <li><a target="_blank" href="<%= sails.config.organization.links.privacyPolicy %>">Privacy Policy</a>|</li>
                    <li class="last-item"><a target="_blank" href="<%= sails.config.organization.links.help %>">Help</a></li>
                </ul>
            </div>

            <script type="text/javascript">

                /////////////////////  HACK ///
                // Mast.isMobile = true;
                ///////////////////// /HACK ///

                Mast.raise({
                    afterRouteFn: function () {
                        if (Mast.isMobile) {
                            Olympus.mobileApp = new Mast.components.AppMobile();
                        }
                    },
                    beforeRouteFn: function () {

                        // If this is mobile, create the mobile app component
                        if (Mast.isMobile) {
                            Olympus = {};

                            // Main app
                            $(".wrapper").remove();
                            $("#footer").remove();

                        }
                        // otherwise, create the components that make up the portal UI
                        else {
                            Olympus = {
                                ui: {
                                    fileSystem: new Mast.components.SuperAdmin({
                                        outlet: '#content'
                                    }),
                                    actionBar: new Mast.components.UploadSearchComponent(),
                                    userNavigation: new Mast.components.UserNavigationComponent()
                                },
                                util: {
                                    dropdownHelper: Mast.DropdownHelper
                                },
                                //isPrivateDeployment: < % - sails.config.privateDeployment % >
                            };

                            // Set active component
                            //Olympus.ui.activeTable = Olympus.ui.fileSystem;
                        }

                    }});
            </script>

            <!-- Template library -->
            <%- rigging.templateLibrary %>
    </body>

    <script type="text/javascript">
        $('.upload-search-template').css({
            'background': "<%=navigation_color %>"
        });
        // $('.listusers-outlet, .log-outlet, .dropdownActions-outlet').css({'background': "<%=body_background %>"});
        //Commented other outlets to avoid admin panel table listing color conflicts
        $('.dropdownActions-outlet').css({'background': "<%=body_background %>"});
        
        //Painted body to color 'Total Space Used' in superadmin
        $('body').css({'background': "<%=body_background %>"});

        $('body').css({'color': "<%=font_color %>"});
        $('a').css({'color': "<%=font_color %>"});
        $('p, label').css({'color': "<%=font_color %>"});
        $('h1, h2, h3, h4, h5, h6').css({'color': "<%=font_color %>"});
        $('p.ajax-loader,p.ajax-loader b,.selected .inode-name,.selected .modified-date').css({'color': "#EFEFEF"});//exception
        $('.files-text, .upload-path-name').css({'color': "#636c78"});//Rishabh: upload/Drive dialog labels

        <% if(font_family !== 'Helvetica, Ariel, sans-serif'){ %>
        $("body, p, a, h1, h2, h3, h4, h5, h6, label, span").css({'font-family': "<%=font_family %>"});
        <%	}else{ %>
        $("body, p, a, h1, h2, h3, h4, h5, h6, label, span").css({'font-family': "ProzimanovaRegular, Arial, Helvetica, sans-serif"});
        <%  } %>
    </script>

    <script type="text/javascript">
        $(".upload-file").hide();
    </script>		

</html>
